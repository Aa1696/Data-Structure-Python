   "source": [
    "def bubblesort(A,count):\n",
    "    for i in range(count):\n",
    "        for j in range(0,len(A)-i-1):\n",
    "            if(A[j]>A[j+1]):\n",
    "                A[j],A[j+1]=A[j+1],A[j]\n",
    "    return A"
   ]
  },
